<Window x:Class="NyamNyam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NyamNyam"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" Background="LightPink" Grid.RowSpan="2">
            <Button Content="Dishes" Margin="0 30 0 0" Click="Dishes_Click" Background="Pink"/>
            <Button Content="Ingredients" Margin="0 30 0 0" Click="Ingredients_Click" Background="Pink"/>
            <Button Content="Orders" Margin="0 30 0 0" Click="Orders_Click" Background="Pink"/>
        </StackPanel>
        <Image Source="C:\Users\WS\Desktop\Sofia\Nyam-Nyam\Nyam-Nyam\Files\Logo.png" HorizontalAlignment="Center" Grid.ColumnSpan="2"/>
        <Frame x:Name="MainFrame" Grid.Column="1" Grid.Row="1"/>
    
    </Grid>
</Window>
